  "main": "lib/webchat/index.js",
  "scripts": {
    "build": "npm run bundle",
    "bundle": "npm run webchat && npm run webchat-legacy && npm run date-picker && npm run date-picker-legacy && npm run rating && npm run rating-legacy &&  npm run speech-input && npm run speech-input-legacy && npm run speech-output && npm run speech-output-legacy && npm run messenger && npm run messenger-legacy && npm run message-renderer",
    "dev": "webpack-dev-server --config webpack.dev.js --host 0.0.0.0",
    "message-renderer": "webpack --config webpack.message-renderer.production.js",
    "date-picker-legacy": "webpack --config webpack.production-plugin.date-picker.legacy.js --entry ./src/plugins/date-picker/index.tsx",
    "date-picker": "webpack --config webpack.production-plugin.date-picker.js --entry ./src/plugins/date-picker/index.tsx",
    "messenger": "webpack --config webpack.production-plugin.messenger.js --entry ./src/plugins/messenger/index.tsx",
    "messenger-legacy": "webpack --config webpack.production-plugin.messenger.legacy.js --entry ./src/plugins/messenger/index.tsx",
    "rating-legacy": "webpack --config webpack.production-plugin.rating.legacy.js --entry ./src/plugins/rating/index.ts",
    "rating": "webpack --config webpack.production-plugin.rating.js --entry ./src/plugins/rating/index.ts",
    "speech-input-legacy": "webpack --config webpack.production-plugin.speech-input.legacy.js --entry ./src/plugins/speech-input/index.tsx",
    "speech-input": "webpack --config webpack.production-plugin.speech-input.js --entry ./src/plugins/speech-input/index.tsx",
    "speech-output-legacy": "webpack --config webpack.production-plugin.speech-output.legacy.js --entry ./src/plugins/speech-output/index.tsx",
    "speech-output": "webpack --config webpack.production-plugin.speech-output.js --entry ./src/plugins/speech-output/index.tsx",
    "webchat-legacy": "webpack --config webpack.production.legacy.js",
    "webchat": "webpack --config webpack.production.js",
    "cypress:open": "cypress open",
    "cypress:serve": "http-server -a localhost -p 8787 dist/",
    "cypress:run": "cypress run",
    "test": "run-p -r cypress:serve cypress:run",
    "pretest": "npm run webchat && npm run date-picker && npm run message-renderer"
  },
